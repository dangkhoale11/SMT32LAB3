#include "fsm_automatic.h"

void fsm_automatic_run(int i){
	switch (status){
	case INIT:
		status = AUTO_RED;
		setTimer(i, timerRed);
		break;
	case AUTO_RED:

		break;
	case AUTO_GREEN:
		break;
	case AUTO_YELLOW:
		break;
	default:
		break;
	}

}
